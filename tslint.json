{
	"extends": [
		"tslint:latest",
		"tslint-config-prettier",
		"tslint-no-circular-imports"
	],
	"rules": {
		"class-name": false,
		"curly": false,
		"member-ordering": false,
		"no-shadowed-variable": false,
		"no-string-literal": false,
		"prettier": true,
		"semicolon": false
	},
	"rulesDirectory": ["tslint-plugin-prettier"]
}
