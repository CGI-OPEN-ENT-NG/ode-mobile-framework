{
  "name": "mobile-app",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "apk": "cd android && ./gradlew assembleRelease",
    "clean": "rm -r -f ./android/app/build && rm -r -f node_modules && rm -f package-lock.json && rm -f yarn.lock && npm install && cd android && ./gradlew clean && cd .. ",
    "cleanGenerated": "rimraf -r -f ./android/app/build && rimraf -f package-lock.json && rimraf node_modules/react-native/local-cli/core/__fixtures__/files/package.json && rimraf -r -f node_modules/react-native-firebase/android/build",
    "fake-server": "./node_modules/.bin/json-server --watch jsonapi/db.json --routes jsonapi/route.json",
    "icons": "react-native link react-native-vector-icons",
    "ios": "react-native run-ios",
    "lang": "./generateLang > ./app/i18n/keyTrans.ts",
    "prettier": "npm run prettier1 && npm run prettier2",
    "prettier1": "prettier-tslint fix 'app/**/*.ts'",
    "prettier2": "prettier-tslint fix 'app/**/*.tsx'",
    "private-key": "cd android/app && rm -f my-release-key.keystore && keytool -genkey -v -keystore my-release-key.keystore -alias my-key-alias -keyalg RSA -keysize 2048 -validity 10000",
    "react-devtools": "react-devtools",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --watch",
    "tsc:build": "./node_modules/.bin/tsc -p tsconfig.json",
    "tsc": "./node_modules/.bin/tsc -p tsconfig.json --watch",
    "tslint": "tslint --project ./tsconfig.json 'app/**/*.{ts,tsx}'",
    "tslint-fix": "tslint --fix --project  ./tsconfig.json 'app/**/*.{ts,tsx}'",
    "upgrade": "react-native-git-upgrade"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "devDependencies": {
    "@types/core-js": "^0.9.45",
    "@types/jest": "^22.1.1",
    "@types/node": "^9.4.5",
    "@types/react": "^16.0.34",
    "@types/react-native": "^0.52.1",
    "@types/react-navigation": "^1.0.31",
    "@types/react-redux": "^5.0.14",
    "@types/redux": "^3.6.31",
    "@types/webpack-env": "^1.13.2",
    "babel-jest": "^22.2.0",
    "babel-preset-react-native": "4.0.0",
    "eslint-plugin-prettier": "^2.5.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^22.1.4",
    "json-server": "^0.12.1",
    "npm-check": "^5.5.2",
    "prettier": "^1.10.2",
    "prettier-tslint": "^0.3.0",
    "react-devtools": "^3.1.0",
    "react-test-renderer": "^16.2.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.5.0",
    "typescript": "2.7.1"
  },
  "dependencies": {
    "fast-html-parser": "1.0.1",
    "glamorous-native": "1.3.0",
    "react": "16.2.0",
    "react-native": "0.53.0",
    "react-native-cookies": "^3.2.0",
    "react-native-fence-html": "1.0.6",
    "react-native-fetch-blob": "0.10.8",
    "react-native-firebase": "3.2.2",
    "react-native-fit-image": "1.5.4",
    "react-native-fs": "2.9.7",
    "react-native-i18n": "2.0.10",
    "react-native-image-to-base64": "git://github.com/SanderVerkuil/react-native-image-to-base64#patch-1",
    "react-native-iphone-x-helper": "1.0.2",
    "react-native-modal": "5.0.0",
    "react-native-smart-splash-screen": "2.3.5",
    "react-native-snap-carousel": "3.5.0",
    "react-native-svg": "6.1.4",
    "react-native-swipeable": "0.6.0",
    "react-native-touchable-scale": "2.0.0",
    "react-native-vector-icons": "4.5.0",
    "react-navigation": "1.0.3",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "rimraf": "2.6.2",
    "typescript": "2.7.1",
    "util": "0.10.3"
  }
