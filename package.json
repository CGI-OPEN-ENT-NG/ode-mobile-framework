{
  "name": "app-e",
  "version": "0.0.9",
  "private": true,
  "ode": {
    "appid": "com.ode.appe",
    "appname": "app-e-dev",
    "properties": {
      "ios": "ios/mobileapp/Info.plist",
      "android": "android/gradle.properties"
    },
    "overrides": [
      "assets/*",
      "android/app/src/main/res/*",
      "ios/mobile-app/Images.xcassets/*"
    ],
    "config": {
      "id": "neo-recette",
      "url": "http://code.web-education.net/nman/mobileapp-config.git",
      "google-services.android.json": "android/app/google-services.json",
      "google-services.ios.plist": "ios/mobileapp/GoogleService-Info.plist",
      "Conf.ts": "app/Conf.ts"
    }
  },
  "scripts": {
    "appcenter:config": "node ode-plugin.js override switch --to=$APPCENTER_TO --dontrestore --acceptall --uri=$APPCENTER_URI --username=$APPCENTER_USERNAME --password=$APPCENTER_PWD",
    "appcenter:build:android": "react-native bundle --platform android --dev false --entry-file 'index.js' --bundle-output 'android/app/src/main/assets/index.android.bundle' --assets-dest 'android/app/build/generated/res/react/release'",
    "appcenter:build:ios": "react-native bundle --platform ios --dev false --entry-file 'index.js' --bundle-output 'ios/mobileapp/main.jsbundle' --assets-dest './ios'",
    "android": "react-native run-android",
    "apk": "cd android && ./gradlew assembleRelease",
    "clean": "rm -r -f ./android/app/build && rm -r -f node_modules && rm -f package-lock.json && rm -f yarn.lock && npm install && cd android && ./gradlew clean && cd .. ",
    "cleanGenerated": "rimraf -r -f ./android/app/build && rimraf -f package-lock.json && rimraf node_modules/react-native/local-cli/core/__fixtures__/files/package.json && rimraf -r -f node_modules/react-native-firebase/android/build",
    "fake-server": "./node_modules/.bin/json-server --watch jsonapi/db.json --routes jsonapi/route.json",
    "ios": "react-native run-ios",
    "prettier": "npm run prettier1 && npm run prettier2",
    "prettier1": "prettier-tslint fix 'app/**/*.ts'",
    "prettier2": "prettier-tslint fix 'app/**/*.tsx'",
    "private-key": "cd android/app && rm -f my-release-key.keystore && keytool -genkey -v -keystore my-release-key.keystore -alias my-key-alias -keyalg RSA -keysize 2048 -validity 10000",
    "react-devtools": "react-devtools",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --watch",
    "tsc:build": "./node_modules/.bin/tsc -p tsconfig.json || true",
    "tsc": "./node_modules/.bin/tsc -p tsconfig.json --watch",
    "tslint": "tslint --project ./tsconfig.json 'app/**/*.{ts,tsx}'",
    "tslint-fix": "tslint --fix --project  ./tsconfig.json 'app/**/*.{ts,tsx}'",
    "upgrade": "react-native-git-upgrade"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "prettier": {
    "semi": true
  },
  "devDependencies": {
    "@types/core-js": "^2.5.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.13",
    "@types/react-native": "^0.57.17",
    "@types/react-navigation": "^2.13.7",
    "@types/react-redux": "^6.0.10",
    "@types/sax": "^1.0.1",
    "@types/webpack-env": "^1.13.6",
    "babel-jest": "^23.6.0",
    "crypto2": "^2.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^23.6.0",
    "json-server": "^0.14.0",
    "metro-react-native-babel-preset": "^0.50.0",
    "npm-check": "^5.9.0",
    "prettier": "^1.15.3",
    "prettier-tslint": "^0.4.0",
    "prompts": "^2.0.1",
    "react-devtools": "^3.4.3",
    "react-native-cli": "^2.0.1",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "^16.6.3",
    "rimraf": "^2.6.2",
    "schedule": "^0.4.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-no-circular-imports": "^0.6.1",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.1",
    "yargs": "^12.0.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "base-64": "^0.1.0",
    "fast-html-parser": "1.0.1",
    "glamorous-native": "1.4.0",
    "i18n-js": "^3.1.0",
    "memoize-one": "^4.0.2",
    "moize": "^5.4.1",
    "moment": "^2.22.2",
    "querystring": "^0.2.0",
    "react": "16.5.0",
    "react-native": "0.57.1",
    "react-native-fast-image": "^5.1.1",
    "react-native-fence-html": "1.0.6",
    "react-native-firebase": "^5.1.1",
    "react-native-fit-image": "1.5.4",
    "react-native-fs": "^2.11.17",
    "react-native-global-props": "1.1.5",
    "react-native-image-picker": "^0.27.1",
    "react-native-image-to-base64": "git://github.com/SanderVerkuil/react-native-image-to-base64#patch-1",
    "react-native-iphone-x-helper": "^1.2.0",
    "react-native-languages": "^3.0.1",
    "react-native-mixpanel": "^1.1.1",
    "react-native-modal": "^6.5.0",
    "react-native-smart-splash-screen": "2.3.5",
    "react-native-snap-carousel": "^3.7.5",
    "react-native-status-bar-height": "^2.1.0",
    "react-native-swipeable": "0.6.0",
    "react-native-touchable-scale": "2.0.0",
    "react-native-vector-icons": "^6.0.0",
    "react-native-view-overflow": "0.0.3",
    "react-navigation": "^2.17.0",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.3.0",
    "rn-fetch-blob": "^0.10.13",
    "sax": "^1.2.4",
    "util": "0.10.3"
  }
}