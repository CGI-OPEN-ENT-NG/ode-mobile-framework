{
  "name": "mobile-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "apk": "cd android && ./gradlew assembleRelease",
    "start": "react-native start",
    "ios": "react-native run-ios",
    "prettier": "npm run prettier1 && npm run prettier2",
    "prettier1": "prettier-tslint fix 'app/**/*.ts'",
    "prettier2": "prettier-tslint fix 'app/**/*.tsx'",
    "private-key": "cd android/app && rm -f my-release-key.keystore && keytool -genkey -v -keystore my-release-key.keystore -alias my-key-alias -keyalg RSA -keysize 2048 -validity 10000",
    "test": "jest --watch",
    "tsc": "tsc -p tsconfig.json --watch"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|native-base|react-navigation|react-native-fabric)"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "devDependencies": {
    "buildfont": "^1.0.10",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^22.0.5",
    "npm-check": "^5.5.2",
    "prettier": "^1.9.2",
    "prettier-tslint": "^0.3.0",
    "react-test-renderer": "^16.2.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "next"
  },
  "dependencies": {
    "@types/core-js": "^0.9.44",
    "@types/react": "^16.0.34",
    "@types/react-native": "^0.51.9",
    "@types/react-navigation": "^1.0.22",
    "@types/react-redux": "^5.0.14",
    "@types/redux": "^3.6.0",
    "axios": "^0.17.1",
    "entcore-toolkit": "^1.0.1",
    "fast-html-parser": "^1.0.1",
    "glamorous-native": "^1.2.1",
    "jsdom": "^11.3.0",
    "moment": "^2.20.1",
    "r": "0.0.5",
    "react": "^16.2.0",
    "react-native": "^0.52.0",
    "react-native-extended-stylesheet": "^0.8.0",
    "react-native-fetch-blob": "git+https://github.com/wkh237/react-native-fetch-blob.git#master",
    "react-native-fs": "^2.9.7",
    "react-native-modal": "^4.1.1",
    "react-native-smart-splash-screen": "^2.3.5",
    "react-native-swipeable": "^0.6.0",
    "react-native-swipeout": "^2.3.1",
    "react-native-vector-icons": "^4.5.0",
    "react-navigation": "^1.0.0-beta.15",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "util": "^0.10.3"
  }
}
